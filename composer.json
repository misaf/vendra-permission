{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "misaf/vendra-permission",
  "description": "Role and permission management for Vendra, built on top of Spatie Permission",
  "type": "vendra-module",
  "license": "MIT",
  "keywords": [
    "laravel",
    "filamentphp",
    "misaf",
    "vendra",
    "permission"
  ],
  "homepage": "https://github.com/misaf/vendra-permission",
  "support": {
    "issues": "https://github.com/misaf/vendra-permission/issues",
    "source": "https://github.com/misaf/vendra-permission"
  },
  "authors": [
    {
      "name": "Ehsan Mahmoodi",
      "email": "misaf.1990@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0",
    "filament/filament": "^4.7.0",
    "laravel/pennant": "^1.19",
    "misaf/vendra-activity-log": "dev-master",
    "misaf/vendra-tenant": "^12.0",
    "misaf/vendra-user": "^12.0",
    "awcodes/filament-badgeable-column": "^3.0",
    "mokhosh/filament-jalali": "^5.1",
    "spatie/laravel-permission": "^7.0.0",
    "spatie/laravel-package-tools": "^1.92"
  },
  "require-dev": {
    "fakerphp/faker": "^1.24.1",
    "larastan/larastan": "^3.9.2",
    "laravel/boost": "^2.0.6",
    "laravel/pint": "^1.27",
    "mockery/mockery": "^1.6.12",
    "nunomaduro/collision": "^8.8.3",
    "orchestra/testbench": "^10.9",
    "pestphp/pest": "^4.3.2",
    "pestphp/pest-plugin-arch": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "pestphp/pest-plugin-livewire": "^4.1",
    "pestphp/pest-plugin-profanity": "^4.2.1",
    "pestphp/pest-plugin-type-coverage": "^4.0.3",
    "misaf/vendra-testing": "dev-master"
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "autoload": {
    "psr-4": {
      "Misaf\\VendraPermission\\": "src/",
      "Misaf\\VendraPermission\\Tests\\": "tests/",
      "Misaf\\VendraPermission\\Database\\Factories\\": "database/factories/",
      "Misaf\\VendraPermission\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Misaf\\VendraPermission\\PermissionServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "../*",
      "options": {
        "symlink": true
      }
    }
  ]
}
